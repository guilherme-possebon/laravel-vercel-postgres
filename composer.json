{
	"name": "guilherme-possebon/laravel-vercel-postgres",
	"description": "A Laravel package to deploy to Vercel with Postgres",
	"keywords": [
		"laravel",
		"vercel",
		"vercel-postgres"
	],
	"homepage": "https://github.com/possebon/laravel-vercel-postgres",
	"license": "MIT",
	"authors": [
		{
			"name": "possebon",
			"email": "gpossebon67@gmail.com",
			"role": "Developer"
		}
	],
"require": {
        "php": "^8.2",
        "illuminate/contracts": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/support": "^12.0"
    },
	"require-dev": {
		"laravel/pint": "^1.0",
		"nunomaduro/collision": "^7.8",
		"spatie/laravel-ray": "^1.26"
	},
	"autoload": {
		"psr-4": {
			"Braumye\\LaravelVercelPostgres\\": "src/"
		}
	},
	"scripts": {
		"format": "vendor/bin/pint"
	},
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"pestphp/pest-plugin": true,
			"phpstan/extension-installer": true
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Braumye\\LaravelVercelPostgres\\LaravelVercelPostgresServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
